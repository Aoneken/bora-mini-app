    "charts": {
        "quarterlyGrowth": {
            "type": "bar",
            "data": {
                "labels": ["Q1 2024", "Q2 2024", "Q3 2024", "Q4 2024", "Q1 2025", "Q2 2025"],
                "datasets": [{
                    "label": "Crecimiento (%)",
                    "data": [5.2, 6.1, 5.8, 7.3, 8.1, 7.9],
                    "backgroundColor": "rgba(79, 70, 229, 0.8)",
                    "borderColor": "rgba(79, 70, 229, 1)",
                    "borderWidth": 1,
                    "borderRadius": 4
                }]
            },
            "options": {
                "responsive": true,
                "plugins": {
                    "legend": { "display": false },
                    "tooltip": {
                        "callbacks": {
                            "label": "function(context) { return ` Crecimiento: ${context.raw}%`; }"
                        }
                    }
                },
                "scales": {
                    "y": {
                        "beginAtZero": true,
                        "ticks": {
                            "callback": "function(value) { return value + '%'; }"
                        }
                    }
                }
            }
        },
        "sectorDistribution": {
            "type": "doughnut",
            "data": {
                "labels": ["Moda", "Electrónica", "Hogar y Jardín", "Salud y Belleza", "Otros"],
                "datasets": [{
                    "label": "Distribución del Mercado",
                    "data": [28, 22, 18, 15, 17],
                    "backgroundColor": [
                        "#4f46e5",
                        "#7c3aed",
                        "#10b981",
                        "#ec4899",
                        "#64748b"
                    ],
                    "hoverOffset": 4
                }]
            },
            "options": {
                "responsive": true,
                "plugins": {
                    "legend": {
                        "position": "bottom"
                    },
                    "tooltip": {
                         "callbacks": {
                            "label": "function(context) { return ` ${context.label}: ${context.raw}%`; }"
                        }
                    }
                }
            }
        }
    }
